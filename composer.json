            "name": "Ali Kiani Nejad",
            "email": "codewithkiani@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
            "ItsKiani\\ConfigParser\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ItsKiani\\ConfigParser\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
